{
    "extends": "@vue/tsconfig/tsconfig.web.json",
    "include": [
        "env.d.ts",
        "./src/**/*.ts",
        "src/**/*",
        "src/**/*.vue",
        "auto-imports.d.ts"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "importsNotUsedAsValues": "remove"
    },
    "references": [
        {
            "path": "./tsconfig.config.json"
        }
    ]
}
